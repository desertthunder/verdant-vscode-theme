    "tokenColors": [
        {
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#9AA0A6",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "constant",
                "entity.name.constant",
                "variable.other.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#006D72"
            }
        },
        {
            "scope": ["entity", "entity.name"],
            "settings": {
